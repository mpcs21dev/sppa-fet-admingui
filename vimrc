set nocompatible
filetype on
filetype plugin on
filetype indent on
syntax on
colorscheme retrobox
set number
set cursorline
set shiftwidth=4 smarttab
set tabstop=4
set expandtab
set nobackup
set nowrap
set incsearch
set ignorecase
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.jpeg,*.png,*.gif,*.mp4

nnoremap <f7> :tabp<CR>
nnoremap <F8> :tabn<CR>
